<?xml version='1.0' encoding='utf-8'?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>FILL THIS IN</h:title>
    <model>
      <instance>
        <faims id="FILL_THIS_IN">
          <!--START: Style-->
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <!--END: Style-->
          <User>
            <User>
              <Select_User/>
            </User>
          </User>
          <Start>
            <Main>
              <Band_Number/>
              <Bird_Species/>
              <Location_Name/>
              <Create_Bird/>
            </Main>
            <Search_Birds>
              <Colgroup_1>
                <Col_1>
                  <Band_Number_1/>
                </Col_1>
                <Col_2>
                  <Band_Number_Measure_1/>
                </Col_2>
                <Col_3>
                  <Search/>
                </Col_3>
              </Colgroup_1>
              <Existing_Birds/>
            </Search_Birds>
            <To_Measure>
              <Birds_to_Measure/>
            </To_Measure>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
              <Entity_Types/>
              <Entity_List/>
            </Search>
            <Map>
              <Map/>
            </Map>
          </Start>
          <Bird>
            <Bird_Info>
              <Band_Number/>
              <Band_Number_Measure/>
              <Bird_Species/>
              <Location_Name/>
              <Colgroup_1>
                <Col_1>
                  <Latitude/>
                </Col_1>
                <Col_2>
                  <Longitude/>
                </Col_2>
              </Colgroup_1>
              <Colgroup_2>
                <Col_1>
                  <Northing/>
                </Col_1>
                <Col_2>
                  <Easting/>
                </Col_2>
                <Col_3>
                  <Accuracy/>
                </Col_3>
              </Colgroup_2>
              <Take_From_GPS_1/>
              <Nest_ID/>
              <Chick_ID/>
              <Age/>
              <Sex/>
              <How_Caught/>
              <Capture_Status/>
              <Bill_Colour/>
              <Bird_timestamp/>
              <Bird_author/>
            </Bird_Info>
            <Measurements>
              <Add_Measurements/>
              <List_of_Measurements/>
            </Measurements>
            <Comments>
              <ID_Pit_Tag/>
              <Colour_Band/>
              <Comments/>
              <Sample/>
              <Attach_Photograph/>
              <Attach_Photograph_Button_1/>
            </Comments>
          </Bird>
          <Measurements>
            <Measurements>
              <Timestamp/>
              <Band_Number/>
              <Band_Number_Measure/>
              <Tarsus_Length/>
              <Head_Length/>
              <Colgroup_1>
                <Col_1>
                  <Bill_Length/>
                </Col_1>
                <Col_2>
                  <Bill_Height/>
                </Col_2>
              </Colgroup_1>
              <Wing_Length/>
              <Tail_Length/>
              <Mass/>
            </Measurements>
          </Measurements>
        </faims>
      </instance>
      <bind nodeset="/faims/Bird/Bird_Info/Band_Number_Measure" type="decimal"/>
      <bind nodeset="/faims/Bird/Comments/ID_Pit_Tag" type="decimal"/>
      <bind nodeset="/faims/Measurements/Measurements/Tarsus_Length" type="decimal"/>
      <bind nodeset="/faims/Measurements/Measurements/Head_Length" type="decimal"/>
      <bind nodeset="/faims/Measurements/Measurements/Colgroup_1/Col_1/Bill_Length" type="decimal"/>
      <bind nodeset="/faims/Measurements/Measurements/Colgroup_1/Col_2/Bill_Height" type="decimal"/>
      <bind nodeset="/faims/Measurements/Measurements/Wing_Length" type="decimal"/>
      <bind nodeset="/faims/Measurements/Measurements/Tail_Length" type="decimal"/>
      <bind nodeset="/faims/Measurements/Measurements/Mass" type="decimal"/>
    </model>
  </h:head>
  <h:body>
    <!--START: Style-->
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <!--END: Style-->
    <group ref="User">
      <label>{User}</label>
      <group ref="User" faims_scrollable="false">
        <label>{User}</label>
        <select1 appearance="compact" ref="Select_User">
          <label>{Select_User}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Start">
      <label>{Start}</label>
      <group ref="Main">
        <label>{Main}</label>
        <select1 ref="Band_Number">
          <label>{Band_Number}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 ref="Bird_Species">
          <label>{Bird_Species}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 ref="Location_Name">
          <label>{Location_Name}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <trigger ref="Create_Bird">
          <label>{Create_Bird}</label>
        </trigger>
      </group>
      <group ref="Search_Birds" faims_scrollable="false">
        <label>{Birds}</label>
        <group ref="Colgroup_1" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <select1 ref="Band_Number_1">
              <label>{Band_Number}</label>
              <item>
                <label>Options not loaded</label>
                <value>Options not loaded</value>
              </item>
            </select1>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input ref="Band_Number_Measure_1">
              <label>{Band_Number_Measure}</label>
            </input>
          </group>
          <group ref="Col_3" faims_style="even">
            <label/>
            <trigger ref="Search">
              <label>{Search}</label>
            </trigger>
          </group>
        </group>
        <select1 appearance="compact" ref="Existing_Birds">
          <label>{Existing_Birds}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="To_Measure" faims_scrollable="false">
        <label>{To_Measure}</label>
        <select1 appearance="compact" ref="Birds_to_Measure">
          <label>{Birds_to_Measure}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>{Search}</label>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input ref="Search_Term">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="large">
            <label/>
            <trigger ref="Search_Button">
              <label>{Search}</label>
            </trigger>
          </group>
        </group>
        <select1 ref="Entity_Types">
          <label>{Entity_Types}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="compact" ref="Entity_List">
          <label>{Entity_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Map">
        <label>{Map}</label>
        <input faims_map="true" ref="Map">
          <label>{Map}</label>
        </input>
      </group>
    </group>
    <group ref="Bird" faims_archent_type="Bird">
      <label>{Bird}</label>
      <group ref="Bird_Info">
        <label>{Bird_Info}</label>
        <select1 faims_attribute_name="Band Number" faims_attribute_type="vocab" ref="Band_Number" faims_style_class="required">
          <label>{Band_Number}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_type="measure" ref="Band_Number_Measure" faims_attribute_name="Band Number">
          <label>{Band_Number_Measure}</label>
        </input>
        <select1 faims_attribute_name="Bird Species" faims_attribute_type="vocab" ref="Bird_Species">
          <label>{Bird_Species}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 faims_attribute_name="Location Name" faims_attribute_type="vocab" ref="Location_Name">
          <label>{Location_Name}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <group ref="Colgroup_1" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Latitude" faims_attribute_type="measure" ref="Latitude" faims_read_only="true">
              <label>{Latitude}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Longitude" faims_attribute_type="measure" ref="Longitude" faims_read_only="true">
              <label>{Longitude}</label>
            </input>
          </group>
        </group>
        <group ref="Colgroup_2" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Northing" faims_attribute_type="measure" ref="Northing" faims_read_only="true">
              <label>{Northing}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Easting" faims_attribute_type="measure" ref="Easting" faims_read_only="true">
              <label>{Easting}</label>
            </input>
          </group>
          <group ref="Col_3" faims_style="even">
            <label/>
            <input faims_attribute_name="Accuracy" faims_attribute_type="measure" ref="Accuracy" faims_read_only="true">
              <label>{Accuracy}</label>
            </input>
          </group>
        </group>
        <trigger ref="Take_From_GPS_1">
          <label>{Take_From_GPS}</label>
        </trigger>
        <input faims_attribute_name="Nest ID" faims_attribute_type="measure" ref="Nest_ID">
          <label>{Nest_ID}</label>
        </input>
        <input faims_attribute_name="Chick ID" faims_attribute_type="measure" ref="Chick_ID">
          <label>{Chick_ID}</label>
        </input>
        <select1 appearance="full" faims_attribute_name="Age" faims_attribute_type="vocab" ref="Age" faims_style_class="required">
          <label>{Age}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="full" faims_attribute_name="Sex" faims_attribute_type="vocab" ref="Sex" faims_style_class="required">
          <label>{Sex}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="full" faims_attribute_name="How Caught" faims_attribute_type="vocab" ref="How_Caught">
          <label>{How_Caught}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select faims_attribute_name="Capture Status" faims_attribute_type="vocab" ref="Capture_Status">
          <label>{Capture_Status}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <select1 type="image" faims_attribute_name="Bill Colour" faims_attribute_type="vocab" ref="Bill_Colour">
          <label>{Bill_Colour}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Bird timestamp" faims_attribute_type="measure" ref="Bird_timestamp" faims_read_only="true">
          <label>{Timestamp}</label>
        </input>
        <input faims_attribute_name="Bird author" faims_attribute_type="measure" ref="Bird_author" faims_read_only="true">
          <label>{Author}</label>
        </input>
      </group>
      <group ref="Measurements" faims_scrollable="false">
        <label>{Measurements}</label>
        <trigger ref="Add_Measurements">
          <label>{Add_Measurements}</label>
        </trigger>
        <select1 appearance="compact" ref="List_of_Measurements">
          <label>{List_of_Measurements}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Comments">
        <label>{Comments}</label>
        <input faims_attribute_name="ID Pit Tag" faims_attribute_type="measure" ref="ID_Pit_Tag" faims_style_class="required">
          <label>{ID_Pit_Tag}</label>
        </input>
        <select1 type="image" faims_attribute_name="Colour Band" faims_attribute_type="vocab" ref="Colour_Band" faims_style_class="required">
          <label>{Colour_Band}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Comments" faims_attribute_type="measure" ref="Comments" faims_style_class="required">
          <label>{Comments}</label>
        </input>
        <select faims_attribute_name="Sample" faims_attribute_type="vocab" ref="Sample" faims_style_class="required">
          <label>{Sample}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <select faims_sync="true" type="camera" faims_attribute_name="Attach Photograph" faims_attribute_type="measure" ref="Attach_Photograph">
          <label>{Attach_Photograph}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <trigger ref="Attach_Photograph_Button_1">
          <label>{Attach_Photograph}</label>
        </trigger>
      </group>
    </group>
    <group ref="Measurements" faims_archent_type="Measurements">
      <label>{Measurements}</label>
      <group ref="Measurements">
        <label>{Measurements}</label>
        <input faims_attribute_name="Timestamp" faims_attribute_type="measure" ref="Timestamp" faims_read_only="true">
          <label>{Timestamp}</label>
        </input>
        <select1 faims_attribute_name="Band Number" faims_attribute_type="vocab" ref="Band_Number" faims_read_only="true">
          <label>{Band_Number}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_type="measure" ref="Band_Number_Measure" faims_attribute_name="Band Number" faims_read_only="true">
          <label>{Band_Number_Measure}</label>
        </input>
        <input faims_attribute_name="Tarsus Length" faims_attribute_type="measure" ref="Tarsus_Length" faims_style_class="required">
          <label>{Tarsus_Length}</label>
        </input>
        <input faims_attribute_name="Head Length" faims_attribute_type="measure" ref="Head_Length" faims_style_class="required">
          <label>{Head_Length}</label>
        </input>
        <group ref="Colgroup_1" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Bill Length" faims_attribute_type="measure" ref="Bill_Length" faims_style_class="required">
              <label>{Bill_Length}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Bill Height" faims_attribute_type="measure" ref="Bill_Height" faims_style_class="required">
              <label>{Bill_Height}</label>
            </input>
          </group>
        </group>
        <input faims_attribute_name="Wing Length" faims_attribute_type="measure" ref="Wing_Length" faims_style_class="required">
          <label>{Wing_Length}</label>
        </input>
        <input faims_attribute_name="Tail Length" faims_attribute_type="measure" ref="Tail_Length" faims_style_class="required">
          <label>{Tail_Length}</label>
        </input>
        <input faims_attribute_name="Mass" faims_attribute_type="measure" ref="Mass" faims_style_class="required">
          <label>{Mass}</label>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
